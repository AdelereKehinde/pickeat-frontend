{
  "expo": {
    "name": "Pickeat",
    "slug": "Pickeat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/logo_png.png",
    "scheme": "pickeat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "devhaywhy"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/logo_png.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oltapp.Pickeat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon/logo_png.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Pickeat to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Pickeat to access your camera",
          "microphonePermission": "Allow Pickeat to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "react-native-vision-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8d931e4d-7f0b-452d-9300-23b9f10f7318"
      }
    }
  }
}
