{
  "expo": {
    "name": "Pickeat",
    "slug": "Pickeat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/logo.png",
    "scheme": "pickeat",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "devhaywhy"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "networkSecurityConfig": {
          "cleartextTrafficPermitted": true
        }
      },
      "package": "com.dev_haywhy.Pickeat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8d931e4d-7f0b-452d-9300-23b9f10f7318"
      }
    }
  }
}
